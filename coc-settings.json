{
  "typescript.showUnused": false,
  "diagnostic.displayByAle": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  },
  "[markdown][css]": {
    "coc.preferences.formatOnSave": true
  },
  "python.formatting.provider": "black",
  "snippets.ultisnips.pythonPrompt": false
}
