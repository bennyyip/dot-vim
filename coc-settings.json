{
  "coc.source.omni.filetypes": [
    "go"
  ],
  "typescript.showUnused": false,
  "diagnostic.displayByAle": true,
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": [
        "c",
        "cpp",
        "cuda",
        "objc",
        "objcpp"
      ],
      "rootPatterns": [
        ".ccls-root",
        "compile_commands.json"
      ],
      "initializationOptions": {
        "cache": {
          "directory": ".ccls-cache"
        }
      }
    }
  },
  "python.linting.pylintArgs": [
    "--rcfile=/data/bennyye/.pylintrc"
  ],
  "snippets.ultisnips.directories": [
    "/home/benyip/.vim/snippets"
  ],
  "snippets.extends": {
    "c": [
      "cpp"
    ],
    "typescript": [
      "javascript"
    ],
    "typescriptreact": [
      "javascript"
    ]
  },
  "snippets.priority": 100,
  "yaml.schemas": {
    "https://cdn-go.cn/orange-ci/orange-ci/latest/conf-schema.json": ".orange-ci.yml"
  }
}
